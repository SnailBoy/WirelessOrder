<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/BrowseDetailContainer"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:baselineAligned="false"
    android:orientation="horizontal" >

    <fragment
        android:id="@+id/Titiles"
        android:layout_width="193dp"
        android:layout_height="match_parent"
        class="com.wirelessorder.fragment.BrowseDetailCatalogFragment" />

    <FrameLayout
        android:id="@+id/Details"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:orientation="horizontal" >
    </FrameLayout>

    <LinearLayout
        android:layout_width="14dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="vertical" >

        <Button
            android:id="@+id/browse_detail_change_browse_way_list_button"
            android:layout_width="fill_parent"
            android:layout_height="120dp" />

        <Button
            android:id="@+id/browse_detail_change_browse_way_gallery_button"
            android:layout_width="fill_parent"
            android:layout_height="120dp" />
    </LinearLayout>

</LinearLayout>